#include "Rectangles.h"



Rectangles::Rectangles(int x,int y,float mass,float velocity,int width,int height): Object(x,y,mass,velocity), m_width(width), m_height(height)
{
}


Rectangles::~Rectangles()
{
}

void Rectangles::Render()
{
	VideoDriver::GetInstance()->DrawRect(m_x, m_y, m_width,m_height);

}
void Rectangles::infor()
{
	std::cout << "Position:" << m_x << "\t" << m_y << std::endl;
	std::cout << "Width, Height:" << m_width <<"\t"<<m_height<< std::endl;
	std::cout << "Mass:" << m_mass << std::endl;
	std::cout << "Velocity:" << m_velocity << std::endl;

}
